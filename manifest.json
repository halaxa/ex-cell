{
  "manifest_version": 2,
  "name": "Ex Cell",
  "version": "0.2.1",
  "content_scripts": [
    {
      "matches": [
        "*://*.sharepoint.com/*",
        "*://*.live.com/*"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "128": "img/128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "declarativeContent"]
}
